<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Company Name</th>
      <th>Mobile no</th>
      <th>Bio</th>
      
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.company_name %></td>
        <td><%= user.mobile_number %></td>
        <td><%= user.bio %></td>

      </tr>
    <% end %>
  </tbody>
</table>


